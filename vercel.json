{
  "version": 2,
  "builds": [
    {
      "src": "frontend",
      "use": "@vercel/static"
    },
    {
      "src": "backend",
      "use": "@vercel/php"
    },
    {
      "src": "installation.sh",
      "use": "@vercel/builders/shell"
    },
    {
      "src": "start-service.sh",
      "use": "@vercel/builders/shell"
    },
    {
      "src": "frontend/dist",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/backend/index.php" },
    { "src": ".*", "dest": "/frontend/index.html" }
  ]
}
