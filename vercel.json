{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "frontend/installation.sh",
      "use": "@vercel/builders/shell"
    },
    {
      "src": "frontend/start-service.sh",
      "use": "@vercel/builders/shell"
    },
    {
      "src": "frontend/dist",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/frontend/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
