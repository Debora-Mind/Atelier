{
  "name": "atelier",
  "version": 2,
  "builds": [
    {
      "src": "start-service.sh", 
      "use": "@vercel/builders/shell"
    },
    {
      "src": "frontend/dist",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "installCommand": "install.sh",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}
